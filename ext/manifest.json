{
  "manifest_version": 2,
  "name": "Focus Dude!!!",
  "description": "Focus Dude!!!, is an Chrome productivity extension to block website access",
  "version": "1.0",
  "browser_action": {
    "default_icon": "not_active_icon.png",
    "default_popup": "popup.html",
    "default_title": "Focus Dude!"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*/*"
  ]
}
